{
  "OpenClaw": {
    "BindAddress": "127.0.0.1",
    "Port": 18789,
    "AuthToken": null,
    "Llm": {
      "Provider": "openai",
      "Model": "gpt-4o",
      "ApiKey": null,
      "Endpoint": null,
      "MaxTokens": 4096,
      "Temperature": 0.7
    },
    "Memory": {
      "StoragePath": "./memory",
      "MaxHistoryTurns": 50,
      "MaxCachedSessions": null
    },
    "Security": {
      "AllowQueryStringToken": false,
      "AllowedOrigins": [],
      "TrustForwardedHeaders": false,
      "KnownProxies": [],
      "AllowUnsafeToolingOnPublicBind": false,
      "AllowPluginBridgeOnPublicBind": false,
      "AllowRawSecretRefsOnPublicBind": false
    },
    "WebSocket": {
      "MaxMessageBytes": 65536,
      "MaxConnections": 1000,
      "MaxConnectionsPerIp": 50,
      "MessagesPerMinutePerConnection": 120
    },
    "Tooling": {
      "AllowShell": true,
      "AllowedReadRoots": [
        "*"
      ],
      "AllowedWriteRoots": [
        "*"
      ]
    },
    "Channels": {
      "Sms": {
        "Twilio": {
          "Enabled": false,
          "AccountSid": null,
          "AuthTokenRef": "env:TWILIO_AUTH_TOKEN",
          "MessagingServiceSid": null,
          "FromNumber": null,
          "WebhookPath": "/twilio/sms/inbound",
          "WebhookPublicBaseUrl": null,
          "ValidateSignature": true,
          "AllowedFromNumbers": [],
          "AllowedToNumbers": [],
          "MaxInboundChars": 2000,
          "RateLimitPerFromPerMinute": 30,
          "AutoReplyForBlocked": false,
          "HelpText": "OpenClaw: reply STOP to opt out."
        }
      },
      "Telegram": {
        "Enabled": false,
        "BotToken": null,
        "BotTokenRef": "env:TELEGRAM_BOT_TOKEN",
        "WebhookPath": "/telegram/inbound",
        "AllowedFromUserIds": []
      },
      "WhatsApp": {
        "Enabled": false,
        "Type": "official",
        "DmPolicy": "pairing",
        "WebhookPath": "/whatsapp/inbound",
        "WebhookVerifyToken": "openclaw-verify",
        "WebhookVerifyTokenRef": "env:WHATSAPP_VERIFY_TOKEN",
        "CloudApiToken": null,
        "CloudApiTokenRef": "env:WHATSAPP_CLOUD_API_TOKEN",
        "PhoneNumberId": null,
        "BridgeUrl": null,
        "BridgeTokenRef": "env:WHATSAPP_BRIDGE_TOKEN"
      }
    },
    "MaxConcurrentSessions": 64,
    "SessionTimeoutMinutes": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  }
}